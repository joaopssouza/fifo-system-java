# Usuário (Tudo antes dos dois pontos da senha)
DB_USERNAME=SEU_USUARIO

# Senha (Entre os dois pontos e o @)
DB_PASSWORD=SUA_SENHA_AQUI

# URL JDBC (O Spring precisa do prefixo 'jdbc:')
